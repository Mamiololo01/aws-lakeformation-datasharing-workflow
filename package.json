  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^1.2.8",
    "@aws-sdk/client-glue": "^3.25.0",
    "@aws-sdk/client-sfn": "^3.25.0",
    "@awsui/collection-hooks": "^1.0.17",
    "@awsui/components-react": "^3.0.216",
